<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <!--根据http://blog.csdn.net/mingwar/article/details/5870431的描述为解决该偶发错误：‘服务器提交了协议冲突. Section=ResponseHeader Detail=CR 后面必须是 LF’ 添加下面一段描述-->
  <system.net>
    <settings>
      <httpWebRequest useUnsafeHeaderParsing="true" />
    </settings>
  </system.net>
 
</configuration>